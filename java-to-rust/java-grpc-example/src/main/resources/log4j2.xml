<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
<appenders>
  <Console name="Console" target="SYSTEM_OUT">
    <PatternLayout pattern="${date}, [${app_name}], [ %-5level ], %logger{10}, %msg %n"/>
  </Console>
</appenders>

<loggers>
  <root level="info">
    <appender-ref ref="Console"/>
  </root>
</loggers>
</configuration>
